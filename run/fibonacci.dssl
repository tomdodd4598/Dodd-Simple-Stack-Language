# ------------------- #
# RECURSIVE FIBONACCI #
# ------------------- #

/f 0 def
/n 0 def

/fib {
	dup 1 > {
		dup 1 - fib
		exch 2 - fib +
	} if
} macro

{
	/n ++
	n 10 > { break } if
	/f n fib =
	f println
} loop

# ------------------- #
# ITERATIVE FIBONACCI #
# ------------------- #

/fib {
	dup 1 > {
		dup 1 - fib
		exch 2 - fib +
	} if
} macro

[ 1 11 ] range new {
	fib println
} foreach
